      <text>手机号</text>
      <view class='lastText color97'>
      <text class='margRight20'>{{phone}}</text>
      <text class='iconfont icon-right setting-icon'></text>
      </view>
    </view>
  </navigator>
 
    <view class='tarInput' bindtap='urlselect'>
      <text>地址</text>
      <view class='lastText color97'>
      <text class='margRight20'>{{fullAddress}}</text>
      <text class='iconfont icon-right setting-icon'></text>
    </view>
    </view>
</view>
