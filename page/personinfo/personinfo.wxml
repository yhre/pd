  <navigator class='navigatorBoder c' url="../changename/changename">
    <view class='tarInput'>
      <text>姓名</text>
       <view class='lastText color97'>
      <text class='margRight20'>{{name}}</text>
      <text class='iconfont icon-right setting-icon'></text>
      </view>
    </view>
  </navigator>
  <navigator class='navigatorBoder c' url="../changephone/changephone">
